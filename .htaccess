# BEGIN WordPress
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.php$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.php [L]
  RewriteCond %{HTTP_REFERER} !^$
  RewriteCond %{HTTP_REFERER} !^http(s)?://(www\.)?google.com [NC]
</IfModule>
# END WordPress



########################################################################################################################
# ========================= TWEAKS =====================================================================================
########################################################################################################################


# BLOCK ACCESS TO DIRECTORIES WITHOUT A DEFAULT DOCUMENT
#
<IfModule mod_autoindex.c>
    Options -Indexes
</IfModule>


# SEND THE CORS HEADER FOR IMAGES WHEN BROWSERS REQUEST IT.
#
<IfModule mod_setenvif.c>
    <IfModule mod_headers.c>
        <FilesMatch "\.(bmp|cur|gif|ico|jpe?g|png|svgz?|webp)$">
            SetEnvIf Origin ":" IS_CORS
            Header set Access-Control-Allow-Origin "*" env=IS_CORS
        </FilesMatch>
    </IfModule>
</IfModule>

# CROSS-ORIGIN WEB FONTS 
#
<IfModule mod_headers.c>
    <FilesMatch "\.(eot|otf|tt[cf]|woff2?)$">
        Header set Access-Control-Allow-Origin "*"
    </FilesMatch>
</IfModule>



# CUSTOM ERROR PAGES
# 
ErrorDocument 400 /security/ERROR400.html
ErrorDocument 401 /security/ERROR401.html
ErrorDocument 403 /security/ERROR403.html
ErrorDocument 404 /security/ERROR404.html
ErrorDocument 405 /security/ERROR405.html
ErrorDocument 414 /security/ERROR414.html
ErrorDocument 416 /security/ERROR416.html
ErrorDocument 500 /security/ERROR500.html
ErrorDocument 501 /security/ERROR501.html
ErrorDocument 502 /security/ERROR502.html
ErrorDocument 503 /security/ERROR503.html
ErrorDocument 504 /security/ERROR504.html


# PREVENTS OVERZEALOUS 404 ERRORS FROM APACHE
#
Options -MultiViews



# TURN OFF DIRECTORY INDEXING (Can break Wordpress' URLs in some situations)

Options All -Indexes


# TURN OFF DIRECTORY INDEXING ALT
#
<IfModule mod_autoindex.c>
  Options -Indexes
</IfModule>


